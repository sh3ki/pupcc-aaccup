        "copy-pdf-worker:bash": "chmod +x scripts/copy-pdf-worker.sh && ./scripts/copy-pdf-worker.sh",
        "copy-pdf-worker:win": "powershell -Command \"New-Item -ItemType Directory -Force -Path 'public/js' >$null; Copy-Item 'node_modules/pdfjs-dist/legacy/build/pdf.worker.mjs' 'public/js/pdf.worker.mjs'; Copy-Item 'node_modules/pdfjs-dist/legacy/build/pdf.worker.min.mjs' 'public/js/pdf.worker.min.js'; if (Test-Path 'node_modules/pdfjs-dist/legacy/build/pdf.worker.js') { Copy-Item 'node_modules/pdfjs-dist/legacy/build/pdf.worker.js' 'public/js/pdf.worker.js' }\"",
        "dev": "vite",
        "format": "prettier --write resources/",
        "format:check": "prettier --check resources/",
        "lint": "eslint . --fix",
        "types": "tsc --noEmit",
        "soketi": "docker rm -f soketi-server 2>NUL & docker run -d -p 6001:6001 -p 9601:9601 -e SOKETI_DEFAULT_APP_ID=pupcc-aaccup-svrehl5a -e SOKETI_DEFAULT_APP_KEY=23277f90ef46e493621dff3390a82a61 -e SOKETI_DEFAULT_APP_SECRET=4daaae0c6de25c382b9cbe3a1ba52aa6 -e SOKETI_DEFAULT_APP_ENABLED=true --name soketi-server quay.io/soketi/soketi:latest"
